<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape = "oval">
            <gradient android:startColor="@color/ivory"
                android:endColor="@color/ivory"
                android:angle="315"/>
        </shape>
    </item>
    <item android:bottom="3dp" android:top="3dp" android:right="3dp" android:left="3dp">
        <shape android:shape="oval">
            <solid android:color = "@color/magentapink"/>
        </shape>
    </item>
    <item android:drawable="@drawable/baseline_add_24"
        android:top="10dp"
        android:bottom="10dp"
        android:right="10dp"
        android:left="10dp"/>
</layer-list>